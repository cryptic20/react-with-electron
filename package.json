  "private": true,
  "author": "Sherwin Wyco",
  "devDependencies": {
    "electron": "^1.8.4",
    "foreman": "^2.0.0",
    "react-scripts": "0.8.5"
  },
  "dependencies": {
    "react": "^16.3.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.3.1"
  },
  "homepage": "./",
